style "scrollbar"
{
  engine "pixmap" 
   {
    image 
      {
        function        = BOX
	recolorable     = TRUE
	detail		= "trough"
	file            = "trough1.png"
	border          = { 6,6,6,6 }
	stretch         = TRUE
	orientation     = VERTICAL
      }
    image 
      {
        function        = BOX
	recolorable     = TRUE
	detail		= "trough"
	file            = "trough2.png"
	border          = { 6,6,6,6 }
	stretch         = TRUE
	orientation     = HORIZONTAL
      }
    image 
      {
        function        = SLIDER
	recolorable     = TRUE
	file            = "scrollbar_vert1.png"
	border          = { 5,5,5,5 }
	stretch         = TRUE
	orientation     = VERTICAL
      }
    image 
      {
        function        = SLIDER
	recolorable     = TRUE
	file            = "scrollbar_horiz1.png"
	border          = { 5,5,5,5 }
	stretch         = TRUE
	orientation     = HORIZONTAL
      }
    image 
      {
        function        = BOX
	recolorable     = TRUE
	state		= PRELIGHT
	file            = "scrollbar_vert1.png"
	border          = { 5,5,5,5 }
	stretch         = TRUE
	orientation     = VERTICAL
      }
    image 
      {
        function        = SLIDER
	recolorable     = TRUE
	file            = "scrollbar_horiz1.png"
	border          = { 5,5,5,5 }
	stretch         = TRUE
	orientation     = HORIZONTAL
      }
    image 
      {
        function        = BOX
	recolorable     = TRUE
	shadow          = IN
	file            = "scrollbar_vert1.png"
	border          = { 5,5,5,5 }
	stretch         = TRUE
	orientation     = VERTICAL
      }
    image 
      {
        function        = SLIDER
	recolorable     = TRUE
	file            = "scrollbar_horiz1.png"
	border          = { 5,5,5,5 }
	stretch         = TRUE
	orientation     = HORIZONTAL
      }
    image 
      {
        function        = BOX
	recolorable     = TRUE
	file            = "scroll1.png"
	border          = { 5,5,5,5 }
	stretch         = TRUE
      }
    }
}

