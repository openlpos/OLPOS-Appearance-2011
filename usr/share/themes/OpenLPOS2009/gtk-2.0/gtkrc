class "GtkSpin*" style "spin"

style "default"
{
  font = "-*-helvetica-medium-r-normal--*-120-*-*-*-*-*-*"
  fg[NORMAL]      = "#000000" 
  fg[PRELIGHT]    = "#ffffff" 
  fg[ACTIVE]      = "#000000" 
  fg[SELECTED]    = "#000000" 
  fg[INSENSITIVE] = "#828282" 
  bg[NORMAL]      = "#878787" 
  bg[PRELIGHT]    = "#878787"
  bg[ACTIVE]      = "#878787"
  bg[SELECTED]    = "#8383a3"
  bg[INSENSITIVE] = "#000000"
  bg_pixmap[NORMAL]    = "background.png"
  base[NORMAL]    = "#A4A4A4"
  base[SELECTED]  = "#8282a3"
  base[ACTIVE]    = "#8383a3"
  GtkRange::slider_width = 15
  GtkScrollbar::min_slider_length = 19
  engine "pixmap" 
  {
    image 
      {
        function        = FOCUS
	recolorable     = TRUE
	overlay_file    = "blank.png"
	overlay_border  = { 0,0,0,0 }
	overlay_stretch = TRUE
      }
    image 
      {
        function        = ARROW
	recolorable     = TRUE
	state           = NORMAL
	file            = "arrow_up1.png"
	stretch         = TRUE 
	arrow_direction = UP
      }
    image 
      {
        function        = ARROW
	recolorable     = TRUE
	state           = PRELIGHT
	file            = "arrow_up1.png"
	stretch         = TRUE 
	arrow_direction = UP
      }
    image 
      {
