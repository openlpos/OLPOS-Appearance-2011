	stretch         = TRUE
      }

    image 
      {
        function        = EXTENSION
	recolorable     = TRUE
	file            = "ext1.png"
	border          = { 8,8,8,8 }
	stretch         = TRUE
      }
    image 
      {
        function        = BOX_GAP
	recolorable     = TRUE
	file            = "up.png"
	gap_file        = "blank.png"
	gap_border      = { 0, 0, 0, 0 }
	gap_start_file  = "blank.png"
	gap_start_border= { 0, 0, 0, 0 }
	gap_end_file    = "blank.png"
	gap_end_border  = { 0, 0, 0, 0 }
      }
  }
}

